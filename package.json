{
  "name": "vfile-reporter-daemon",
  "version": "1.0.0",
  "keywords": [
    "lsp",
    "daemon",
    "vfile"
  ],
  "license": "MIT",
  "author": "Luma",
  "scripts": {
    "fix": "npm run fix:eslint",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint . --ext .js,.ts,.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:prod": "run-s build:prod:*",
    "build:prod:0": "rimraf dist",
    "build:prod:1": "tsc",
    "build:dev": "etsc",
    "watch": "nodemon -w src -e ts,js -x npm run dev:rebuild-restart",
    "dev:rebuild-restart": "run-s dev:rebuild-restart:*",
    "dev:rebuild-restart:0": "npm run build:dev",
    "dev:rebuild-restart:1": "node ./dist/bin/vfile-reporter-daemon.js stop",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "core_d": "^3.0.0"
  },
  "devDependencies": {
    "better-typescript-lib": "^1.0.1",
    "esbuild": "^0.8.32",
    "esbuild-node-tsc": "^1.0.5",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-airbnb-typescript-prettier": "^4.1.0",
    "eslint-config-import": "^0.13.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3",
    "vfile": "^4.2.1"
  }
}
